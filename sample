// src/app/product-search.interface.ts

export interface ProductSearch {
  productName?: string;
  color?: string;
  priceRange?: { min: number; max: number };
  category?: string;
}

export interface Product {
  productId: number;
  productActivationDate: Date;
  productName: string;
  color: string;
  price: number;
  category: string;
}
